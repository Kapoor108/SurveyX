{
  "name": "org-survey-app",
  "version": "1.0.0",
  "description": "Organizational Survey Application",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install:all": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
